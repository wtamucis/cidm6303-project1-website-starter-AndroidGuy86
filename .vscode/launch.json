{
  "version": "0.2.0",
  "configurations": [
    
    {
      "name": "Chrome + Live Server",
      "type": "pwa-chrome",
      "request": "launch",
      "url": "http://127.0.0.1:5500",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true
    },
    {
      "name": "Edge + Live Server",
      "type": "pwa-msedge",
      "request": "launch",
      "url": "http://127.0.0.1:5500",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true
    },
    {
      "name": "Firefox + Live Server",
      "type": "pwa-firefox",
      "request": "launch",
      "url": "http://127.0.0.1:5500",
      "webRoot": "${workspaceFolder}",
      "sourceMaps": true
    },
    {
      "name": "Safari + Live Server",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "open",
      "runtimeArgs": [
        "-a",
        "Safari",
        "http://127.0.0.1:5500"
      ]
    }
  ]
}